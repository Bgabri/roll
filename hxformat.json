{
    "disableFormatting": false,
    "emptyLines": {
        "importAndUsing": {
            "beforeType": 1,
            "beforeUsing": 1,
            "betweenImports": 1,
            "betweenImportsLevel": "firstLevelPackage"
        }
    },
    "indentation": {
        "indentObjectLiteral": true,
        "tabWidth": 4,
        "character": "    "
    },
    "lineEnds": {
        "blockCurly": {
            "leftCurly": "after",
            "emptyCurly": "noBreak"
        }
    },
    "sameLine": {
        "ifBody": "same",
        "elseBody": "same",
        "ifElse": "same",
        "elseIf": "same",
        "expressionIf": "same",
        "expressionIfWithBlocks": false,
        "functionBody": "same",
        "anonFunctionBody": "same"
    },
    "whitespace": {},
    "wrapping": {
        "maxLineLength": 80,
        "callParameter": {
            "defaultWrap": "noWrap",
            "rules": [
                {
                    "conditions": [
                        {
                            "cond": "itemCount <= n",
                            "value": 0
                        }
                    ],
                    "type": "noWrap"
                },
                {
                    "conditions": [
                        {
                            "cond": "allItemLengths <= n",
                            "value": 30
                        }
                    ],
                    "type": "noWrap"
                },
                {
                    "conditions": [
                        {
                            "cond": "exceedsMaxLineLength",
                            "value": 1
                        }
                    ],
                    "type": "fillLineWithLeadingBreak"
                }
            ]
        },
        "functionSignature": {
            "defaultAdditionalIndent": 1,
            "defaultWrap": "noWrap",
            "rules": [
                {
                    "conditions": [
                        {
                            "cond": "itemCount >= n",
                            "value": 8
                        }
                    ],
                    "type": "onePerLine"
                },
                {
                    "conditions": [
                        {
                            "cond": "exceedsMaxLineLength",
                            "value": 1
                        }
                    ],
                    "type": "fillLineWithLeadingBreak"
                }
            ]
        },
        "methodChain": {
            "defaultWrap": "onePerLineAfterFirst",
            "rules": [
                {
                    "conditions": [
                        {
                            "cond": "itemCount <= n",
                            "value": 2
                        },
                        {
                            "cond": "exceedsMaxLineLength",
                            "value": 0
                        }
                    ],
                    "type": "noWrap"
                },
                {
                    "conditions": [
                        {
                            "cond": "exceedsMaxLineLength",
                            "value": 1
                        }
                    ],
                    "type": "onePerLineAfterFirst"
                }
            ]
        },
        "multiVar": {
            "defaultWrap": "onePerLine"
        },
        "opAddSubChain": {
            "defaultWrap": "noWrap",
            "rules": [
                {
                    "conditions": [
                        {
                            "cond": "exceedsMaxLineLength",
                            "value": 1
                        }
                    ],
                    "type": "onePerLine"
                }
            ]
        },
        "opBoolChain": {
            "defaultWrap": "noWrap",
            "rules": [
                {
                    "conditions": [
                        {
                            "cond": "itemCount >= n",
                            "value": 4
                        }
                    ],
                    "type": "onePerLine"
                },
                {
                    "conditions": [
                        {
                            "cond": "exceedsMaxLineLength",
                            "value": 1
                        }
                    ],
                    "type": "onePerLine"
                }
            ]
        }
    }
}